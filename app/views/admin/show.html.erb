<h1>Admin Dashboard</h1>
<%= link_to "Admin Merchant Index", "/admin/merchants" %>
<%= link_to "Admin Invoice Index", "/admin/invoices" %>

<h3>Incomplete Invoices</h3>
<% @invoices.incomplete_invoices_ordered.each do |invoice| %>
  <%= link_to "#{invoice.id}", "/admin/invoices/#{invoice.id}" %>
  <%= " - #{invoice.created_at.strftime("%A, %B %d, %Y")}" %>
  <br>
<% end %>
